___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
log('data =', data);

if (!data.company_id) {
  log("Missing 'company_id' in the data");
  data.gtmOnFailure();
  return;
}

const queryPermission = require('queryPermission');
const injectScript = require('injectScript');
const callInWindow = require('callInWindow');

const url = "https://www.huren.nl/tag_server/js?advertiser_id="+data.company_id;

var event_context = {};

if (data.value !== undefined) {
  event_context.value = data.value;
}

if (data.transaction_id !== undefined) {
  event_context.transaction_id = data.transaction_id;
}

log('Event context', event_context);

function onSuccess(){
  log("Success, call event");
  callInWindow('hnl_event', data.event_type, event_context);  
  data.gtmOnSuccess();
}

if (queryPermission('inject_script', url)) {
  if (queryPermission('access_globals', 'execute', 'hnl_event')) {
    log('Inject script');
    injectScript(url, onSuccess, data.gtmOnFailure);
  } else {
    log('Permission failure');
    data.gtmOnFailure();
  }
} else {
  log('Permission failure');
  data.gtmOnFailure();
}


